{% set pickupPointId = shipment.pickupPointId %}

{% if pickupPointId %}
    <label class="setono-sylius-pickup-point-label"
           data-pickup-point-id="{{ pickupPointIdentifier }}"
           data-url="{{ path('setono_sylius_pickup_point_shop_ajax_pickup_point_by_id', { 'pickupPointId': pickupPointIdentifier }) }}"
           style="display: none;">
        {{ 'setono_sylius_pickup_point.ui.shipment_to'|trans }}
    </label>
{% endif %}
